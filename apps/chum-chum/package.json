{
  "name": "chum-chum",
  "version": "0.0.0",
  "scripts": {
    "build": "next build",
    "dev": "open http://localhost:3100 && next dev --turbo --port 3100",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "tsc": "tsc -p tsconfig.json",
    "format-fix": "prettier --config ../../.prettierrc --write --ignore-unknown -l .",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watch",
    "test-coverage": "jest --coverage --verbose",
    "storybook": "start-storybook -p 6100 --quiet",
    "storybook-build": "build-storybook -o build-storybook-static",
    "update-dependencies": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@trpc/client": "^10.6.0",
    "@trpc/next": "^10.6.0",
    "@trpc/react-query": "^10.6.0",
    "@trpc/server": "^10.6.0",
    "next-auth": "^4.18.7",
    "superjson": "1.12.1",
    "zod": "^3.20.2",
    "ui": "*",
    "utils-version": "*",
    "database": "*"
  },
  "devDependencies": {
    "config-storybook": "*",
    "config-tsconfig": "*",
    "utils-test": "*"
  }
}
